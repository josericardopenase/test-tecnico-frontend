ts/SideBar/index.js",[],[],"/home/juan/Projects/mios/test-tecnico-frontend/src/common/components/Player/index.js",[],[],"/home/juan/Projects/mios/test-tecnico-frontend/src/common/components/Header/Header.js",[],[],"/home/juan/Projects/mios/test-tecnico-frontend/src/common/components/Player/Player.js",[],[],"/home/juan/Projects/mios/test-tecnico-frontend/src/common/components/SideBar/SideBar.js",[],[],"/home/juan/Projects/mios/test-tecnico-frontend/src/routes/Discover/components/Discover.js",[],[],"/home/juan/Projects/mios/test-tecnico-frontend/src/routes/Discover/components/DiscoverBlock/components/DiscoverBlock.js",[],[],"/home/juan/Projects/mios/test-tecnico-frontend/src/routes/Discover/components/DiscoverBlock/components/DiscoverItem.js",[],[],"/home/juan/Projects/mios/test-tecnico-frontend/src/api/index.js",[],[],"/home/juan/Projects/mios/test-tecnico-frontend/src/api/Spotify/index.js",["85"],[],"import api from '../../config.js'\nimport axios from 'axios';\n\nconst client = axios.create({ baseURL: api.api.baseUrl, json: true });\n\nconst tokenExpired = token => Object.keys(token).length === 0 || token.expired + token.timeInSec < 30\n\nconst getToken = () => {\n  const token = localStorage.getItem('token')\n  if(token){\n    try{\n      return JSON.parse(token)\n    }catch(err){\n      return {}\n    }\n  }else{\n    return {}\n  }\n}\n\nclient.interceptors.request.use((config) => {\n  const token = getToken()\n  if (tokenExpired(token)) {\n    auth();\n  }\n  return config;\n});\n\n\nconst auth = () => {\n  const formData = new URLSearchParams()\n  formData.append(\"grant_type\", \"client_credentials\")\n  const options = {\n        headers: {\n          'Content-Type': 'application/x-www-form-urlencoded'\n        },\n        auth: {\n          username: api.api.clientId,\n          password: api.api.clientSecret\n        },\n        json: true\n  }\n  axios.post(api.api.authUrl, formData, options).then(function(response) {\n          let token = Object.assign({}, response.data);\n          token.timeInSec = Math.floor(Date.now() / 1000);\n          localStorage.setItem('token', JSON.stringify(token))\n      }).catch(function(err) {\n         throw new Error(`HTTP Error on auth token ${err}`)\n      })\n}\n\nexport default {\n    async execute(method, resource, respObj, data) {\n        const token = getToken()\n        return client({\n          method,\n          url: resource,\n          data,\n          crossdomain: true ,\n          headers: { \"Authorization\": `Bearer ${token.access_token}` }\n        }).then(req => {\n          return req.data[respObj].items\n        })\n      },\n\n      getNewReleases() {\n        return this.execute('get', 'browse/new-releases', 'albums')\n      },\n\n      getFeaturedPlaylists() {\n        return this.execute('get', 'browse/featured-playlists', 'playlists')\n      },\n\n      getCategories() {\n        return this.execute('get', 'browse/categories', 'categories')\n      },\n\n}\n","/home/juan/Projects/mios/test-tecnico-frontend/src/config.js",["86"],[],"export default {\n  api: {\n    baseUrl: 'https://api.spotify.com/v1',\n    authUrl: 'https://accounts.spotify.com/api/token',\n    clientId: process.env.REACT_APP_SPOTIFY_CLIENT_ID,\n    clientSecret: process.env.REACT_APP_SPOTIFY_CLIENT_SECRET\n  }\n}\n",{"ruleId":"87","severity":1,"message":"88","line":52,"column":1,"nodeType":"89","endLine":78,"endColumn":2},{"ruleId":"87","severity":1,"message":"88","line":1,"column":1,"nodeType":"89","endLine":8,"endColumn":2},"import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration"]